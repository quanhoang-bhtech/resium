{
  "name": "resium",
  "version": "1.10.1",
  "description": "React components for Cesium",
  "main": "dist/resium.cjs.js",
  "module": "dist/resium.es.js",
  "types": "dist/types/src/index.d.ts",
  "repository": "https://github.com/darwin-education/resium.git",
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "test": "jest --coverage",
    "clean": "del dist coverage examples/build",
    "clean:build": "del dist",
    "type": "tsc --noEmit && yarn --cwd docs type",
    "lint": "eslint --ext '.js,.ts,.tsx' . && yarn --cwd docs lint",
    "build": "run-s clean:build build:es build:cjs build:umd build:umd:min",
    "build:es": "cross-env NODE_ENV=es rollup -c",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build:umd": "rollup -c",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c",
    "format": "eslint --ext '.js,.ts,.tsx' --fix . && yarn --cwd docs format",
    "storybook": "start-storybook -s .storybook/public -p 9001",
    "storybook:build": "build-storybook -s .storybook/public -o docs/.docz/public/examples",
    "doc": "yarn --cwd docs dev",
    "doc:build": "yarn --cwd docs build && build-storybook -o docs/.docz/public/examples",
    "doc:generate": "yarn --cwd docs generate"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=8.3.0"
  },
  "peerDependencies": {
    "cesium": "1.x",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "@rollup/plugin-replace": "2.3.1",
    "@storybook/addon-actions": "5.3.12",
    "@storybook/addon-storysource": "5.3.12",
    "@storybook/react": "5.3.12",
    "@storybook/theming": "5.3.12",
    "@types/cesium": "1.59.6",
    "@types/enzyme": "3.10.5",
    "@types/jest": "25.1.2",
    "@types/lodash.omit": "4.5.6",
    "@types/lodash.pick": "4.4.6",
    "@types/node": "13.7.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "babel-loader": "8.0.6",
    "cesium": "1.65.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "7.0.0",
    "del-cli": "3.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "file-loader": "5.0.2",
    "jasmine-core": "3.5.0",
    "jest": "25.1.0",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rollup": "1.31.0",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.5.0",
    "rollup-plugin-terser": "5.2.0",
    "rollup-plugin-typescript2": "0.25.3",
    "ts-jest": "25.2.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "webpack": "4.41.5"
  },
  "dependencies": {
    "facepaint": "^1.2.1"
  }
}
